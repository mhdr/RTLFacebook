{
  "manifest_version": 2,
  "name": "RTL Facebook",
  "version": "0.2",
  "icons": { "16": "facebook16.png",
           "48": "facebook48.png",
          "128": "facebook128.png" },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": ["jquery.min.js", "content.js"],
      "css":["fb.css"]
    }
  ],
  "web_accessible_resources": ["fb.css"]
}
